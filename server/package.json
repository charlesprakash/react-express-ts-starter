{
    "name": "yatner-server",
    "version": "1.0.0",
    "dependencies": {
        "body-parser": "^1.18.2",
        "express": "^4.16.2",
        "morgan": "^1.9.0"
    },
    "scripts": {
        "start": "node dist/index.js",
        "build": "tsc",
        "test": "jest --forceExit",
        "watch": "concurrently --kill-others \"tsc -w\" \"nodemon dist/index.js\""
    },
    "devDependencies": {
        "@types/body-parser": "^1.16.8",
        "@types/debug": "^0.0.30",
        "@types/express": "^4.0.39",
        "@types/jest": "^21.1.8",
        "@types/morgan": "^1.7.35",
        "@types/node": "^8.0.58",
        "@types/supertest": "^2.0.4",
        "concurrently": "^3.5.1",
        "debug": "^3.1.0",
        "jest": "^21.2.1",
        "jest-cli": "^21.2.1",
        "nodemon": "^1.13.3",
        "supertest": "^3.0.0",
        "ts-jest": "^21.2.4",
        "ts-node": "^4.0.1",
        "tslint": "^5.8.0",
        "tslint-react": "^3.2.0",
        "typescript": "^2.6.2"
    },
    "jest": {
        "transform": {
            "^.+\\.tsx?$": "ts-jest",
            "^.+\\.ts?$": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?|ts?|js?)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    }
}